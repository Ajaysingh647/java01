public class prashant {
    public static void main(String[] args) {
        System.out.println("The sum is not user defined");
        System.out.print("the sum of three number is :- ");
        int x = 2;
        int y = 3;
        int z = 4;
        int sum = x + y + z;
        System.out.println(sum);
    }
}
